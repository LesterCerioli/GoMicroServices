}

type ContractKeyOptions {
	Key         string             `json:"key"`                  // 属性key值
	Label       string             `json:"label"`                // 属性名称
	Type        string             `json:"type"`                 // 属性数据类型
	Value       string             `json:"value,optional"`       // 属性值
	ShowType    string             `json:"showType"`             // 属性展现方式
	Unit        string             `json:"unit,optional"`        // 属性单位
	Description string             `json:"description,optional"` // 属性描述信息
	Default     string             `json:"default,optional"`     // 属性默认值
	Rules       []*RulesOptions    `json:"rules,optional"`       // 对key值的验证规则
	Format      string             `json:"format,optional"`      // format
	Properties  *PropertiesOptions `json:"properties,optional"`  // 自定义属性
	Operations  []string           `json:"operations"`           // 操作
}

