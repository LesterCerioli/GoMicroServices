}

type RulesOptions {
	Required bool   `json:"required,optional"` // 是否必填
	Type     string `json:"type,optional"`     // key对应值类型
	Message  string `json:"message,optional"`  // 验证错误信息
	Trigger  string `json:"trigger,optional"`  // 触发验证时机
}

type SelectOptions {
	Id   int64  `json:"id"`   // id
	Name string `json:"name"` // 展示名字
}

type TableHeaderOptions {
	Prop  string `json:"prop"`  // 列绑定的属性值
	Label string `json:"label"` // 列名
}

type ShowTypesOptions {
	Select          int64                            `json:"select,optional"`          // 已选择
	SelectOptions   []*SelectOptions                 `json:"selectOptions,optional"`   // 下拉选项
	Radio           int64                            `json:"radio,optional"`           // 单选，已选择
	RadioOptions    []*SelectOptions                 `json:"radioOptions,optional"`    // 单选项
	CheckBox        []int64                          `json:"checkBox,optional"`        // 复选框，已选择
	CheckBoxOptions []*SelectOptions                 `json:"checkBoxOptions,optional"` // 复选框选项
	TableHeader     []*TableHeaderOptions            `json:"tableHeader,optional"`     // 表头信息
	TableData       []map[string]*ContractKeyOptions `json:"tableData,optional"`       // 数据信息 一行信息
	Module          []int64                          `json:"module,optional"`          // 已选择
	ModuleOptions   []SelectOptions                  `json:"moduleOptions"`            // 模块选项
}

type PropertiesOptions {
	PlaceHolder string            `json:"placeHolder,optional"` // 输入提示信息
	Tip         string            `json:"tip,optional"`         // 属性提示信息
