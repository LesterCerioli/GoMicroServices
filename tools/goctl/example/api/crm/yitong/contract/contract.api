}

type PaybackDetails {
	TotalAmount   int64            `json:"totalAmount"`   // 回款总额，放大100倍
	PaybackDetail []*PaybackDetail `json:"paybackDetail"` // 回款信息
}

type PaybackDetail {
	Id     int64  `json:"id"`     // 回款Id
	Remark string `json:"remark"` // 回款记录
	Amount int64  `json:"amount"` // 回款金额，放大100倍
	Date   int64  `json:"date"`   // 回款时期
}

type PaymentEditRequest {
	PaymentId  int64   `json:"paymentId,range=[1:]"` // 回款Id
	Content    string  `json:"content"`              // 回款内容
	Amount     int64   `json:"amount"`               // 回款金额
	Date       int64   `json:"date"`                 // 回款时间
