type PayDetail {
	Amounts int64 `json:"amounts"`
	Date    int64 `json:"date"`
}

type ExpressDetails {
	CampusSealFirst    bool           `json:"campusSealFirst"`    // 是否校方先盖章
	CampusExpressInfo  *ExpressDetail `json:"campusExpressInfo"`  // 合同回寄地址
	CompanyExpressInfo *ExpressDetail `json:"companyExpressInfo"` // 我司收件地址
}

type ExpressDetail {
	ExpressId        int64  `json:"expressId,optional"`
	ContractId       int64  `json:"contractId,optional"`
	ReceiptType      string `json:"receiptType"`    // CAMPUS校方收；COMPANY公司收
	ExpressType      int64  `json:"expressType"`    // 1、邮寄；2、自取/自送
	RecipientName    string `json:"recipientName"`  // 收件人
	RecipientPhone   string `json:"recipientPhone"` // 收件人联系方式
	RecipientAddress string `json:"recipientAddress,optional"`
	ExpressName      string `json:"expressName,optional"`
	ExpressNumber    string `json:"expressNumber,optional"`
	FirstExpressTime int64  `json:"firstExpressTime,optional"`
	ExpressTime      int64  `json:"expressTime,optional"`
}

type InvoiceDetails {
	TotalAmount   int64            `json:"totalAmount"` // 开票总额，放大100倍
	InvoiceDetail []*InvoiceDetail `json:"invoiceDetail"`
}

type InvoiceDetail {
	InvoiceId   int64  `json:"invoiceId"`   // 发票ID
	ApplyDate   int64  `json:"applyDate"`   // 申请开票时间
	Title       string `json:"title"`       // 开票抬头
	Amount      int64  `json:"amount"`      // 金额，放大100倍
	Status      string `json:"status"`      // 合同状态
