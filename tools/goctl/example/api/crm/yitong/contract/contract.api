}

type CancelContractRequest {
	ContractId int64  `json:"contractId,optional"`
	Remarks    string `json:"remarks,optional"`
}

@server(
	host: 0.0.0.0
	port: 2019
	jwt: Auth
)
service contract {
	@doc(
		summary: PC端合同详情
	)
	@server(
		handler: getContractDetail
	)
	post /contract/detail(ContractDetailRequest) returns(ContractDetailsResponse)
	
	@doc(
		summary: 获取寄送信息
	)
	@server(
		handler: getExpressDetail
	)
	get /contract/express/:contractId(ExpressDetailRequest) returns(ExpressDetails)
	
	@doc(
		summary: 编辑寄送信息
	)
	@server(
		handler: editExpress
	)
	post /contract/express/edit(ExpressEditRequest)
	
	@doc(
		summary: 判断寄送信息是否可编辑
	)
	@server(
		handler: checkExpress
	)
	post /contract/express/check(ExpressCheckRequest) returns(BoolResponse)
	
	@doc(
		summary: 获取产品配置信息
	)
	@server(
		handler: getProductProperty
	)
	post /contract/product/property(PropertyRequest) returns(PropertyResponse)
	
	@doc(
		summary: 搜索寄送信息
	)
	@server(
		handler: contractHandler
	)
	post /contract/express/search(ExpressSearchRequest) returns(ExpressSearchResponse)
	
	@doc(
		summary: 回款编辑
	)
	@server(
		handler: editPayment
	)
	post /contract/payment/edit(PaymentEditRequest) returns(PaymentResponse)
	
	@doc(
		summary: 回款删除
	)
	@server(
		handler: deletePayment
	)
	post /contract/payment/delete(PaymentDeleteRequest) returns(PaymentResponse)
	
	@doc(
		summary: 通信地址列表
	)
	@server(
		handler: siteList
	)
	post /contract/site/list(SiteCondition) returns(SiteDetailResponse)
	
	@doc(
		summary: 项目类型列表
	)
	@server(
		handler: getRealProjectList
	)
	get /contract/project/list() returns(ProjectList)
	
	@doc(
		summary: 合同模板列表
	)
	@server(
		handler: contractTemplateList
	)
	post /contract/template/list(ContractTemplateListRequest) returns(ContractTemplateListResponse)
	
	@doc(
		summary: 客户
	)
	@server(
		handler: privateSeaSearch
	)
	post /school/private-sea/search(PublicSeaSearchRequest) returns(NewSearchNextResponse)
	
	@doc(
		summary: 新增/编辑合同
	)
	@server(
		handler: addContract
	)
	post /contract/add(AddContractRequest) returns(AddContractResponse)
	
	@doc(
		summary: app首页大数据-已回款列表-个人
	)
	@server(
		handler: personPaymentList
	)
	post /contract/big-data/person-payment-list(BigDataPaymentListRequest) returns(ContractDetail)
	
	@doc(
		summary: app首页大数据-已回款列表-团队
	)
	@server(
