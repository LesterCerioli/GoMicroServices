	EvaluationTypes []int  `json:"evaluationTypes"` // 当前任务下包含的指标类型
	SyncAppraisal   int64  `json:"syncAppraisal"`   // 是否同步学生评价 1同步 0不同步
}

type TaskConfigListResponse {
	ConfigList []*ConfigInfo `json:"configList"` // 配置列表
	Avatars    []string      `json:"avatars"`    // 任务图标列表
}

type ConfigInfo {
