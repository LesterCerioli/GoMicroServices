}

type EvaluateTaskContent {
	Id              int64                      `json:"id,optional"`
	ContentName     string                     `json:"contentName"`      // 指标内容名称
	SelectType      int                        `json:"selectType"`       // 1 单选 2多选
	Status          int                        `json:"status,omitempty"` // 不处理:0 insert:1 update:2 delete:3
