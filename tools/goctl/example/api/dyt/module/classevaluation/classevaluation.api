type EvaluationTaskListResponse {
	List []*EvaluationTask `json:"list"` // 可评价任务列表 app端
}

type EvaluationTask {
	TaskId           int64  `json:"taskId,omitempty"`          // 任务 Id
	TaskName         string `json:"taskName,omitempty"`        // 任务名称
	Avatar           string `json:"avatar,omitempty"`          // 任务图标
	EvaluationTypes  []int  `json:"evaluationTypes,omitempty"` // 可评价的类型
	Frequency        int64  `json:"frequency"`                 // 频次 0: 日， 1：周， 2：月
	CalculationCycle int64  `json:"calculationCycle"`          // 计算周期  1: 日， 2：周， 3：月
	CreateTime       int64  `json:"createTime,omitempty"`
}

type EvaluateGridRequest {
	TaskId       int64 `json:"taskId,omitempty"`             // 任务 Id
	EvaluateType int   `json:"evaluateType,omitempty"`       // 别评价对象类型 1：班级 2：地点  3：老师
	StartTime    int64 `json:"startTime,omitempty,optional"` // 开始时间
	EndTime      int64 `json:"endTime,omitempty,optional"`   // 结束时间
}

type EvaluateGridResponse {
	IsManager        bool                   `json:"isManager"`
	IsEvaluator      bool                   `json:"isEvaluator"`
	Indicators       []*GridIndicator       `json:"indicators,omitempty"`
	EvaluatedObjects []*GridEvaluatedObject `json:"evaluatedObjects,omitempty"`
}

type GridIndicator {
	Id        int64                   `json:"id,omitempty"`                 // 指标 Id
	Name      string                  `json:"name,omitempty,optional"`      // 指标名称
	BaseScore int                     `json:"baseScore,omitempty,optional"` // 基础分数
	Sort      int64                   `json:"sort,omitempty"`               // 排序
	Contents  []*GridIndicatorContent `json:"contents,omitempty"`           // 指标内容
}

type GridIndicatorContent {
	Id         int64  `json:"id,omitempty"`            // 内容 Id
	Name       string `json:"name,omitempty,optional"` // 内容名称
	SelectType int    `json:"selectType,omitempty"`    // 是否可多选 1：单选 2：多选
}

type GridEvaluatedObject {
	Type        int                    `json:"type,omitempty,optional"`        // 被评价对象类型 1：班级 2：地点  3：老师
	EvaluatedId int64                  `json:"evaluatedId,omitempty"`          // 被评价对象 1：班级 Id 2：地点 Id  3：老师 Id
	DisplayName string                 `json:"displayName,omitempty,optional"` // 显示的名称
	Pinyin      string                 `json:"pinyin,optional"`
	IsEvaluator bool                   `json:"isEvaluator"`
	Cells       []*GridEvaluatedStatus `json:"cells,omitempty"` // 评价状态
}

type GridEvaluatedStatus {
	IndicatorId int64 `json:"indicatorId,omitempty"` // 对应 GridIndicator 中 Id
	ContentId   int64 `json:"contentId,omitempty"`   // 对应 GridIndicatorContent 中 Id
	Status      int   `json:"status,omitempty"`      // 评价的状态 0：不可评价  1：未评价  2： 已评价
	HasScore    int   `json:"hasScore,omitempty"`    // 是否在评价周期之内有分数 0: 没有，  1： 有
	Score       int   `json:"score,omitempty"`       // 已经评价分数
	IsEvaluator int   `json:"is_evaluator"`          // 是否为评价者
}

type EvaluateAddLogRequestV3 {
	DetailId        int64              `json:"detailId,optional"`        // 信息id
	TaskId          int64              `json:"taskId"`                   // 任务id
	IndicatorId     int64              `json:"indicatorId"`              // 指标id
	ContentId       int64              `json:"contentId"`                // 内容id
	EvaluateType    int64              `json:"evaluateType"`             // 评价类型
	Objects         []*EvaluateObjects `json:"objects"`                  // 评价对象
	Remark          *CommentDetail     `json:"remark,optional"`          // 评价内容
	ContentItemList []*ContentItemInfo `json:"contentItemList,optional"` // 评价项列表
	IsTellParent    int64              `json:"isTellParent,optional"`    // 是否通知家长 1 通知，0 不通知
}

type CommentDetail {
	ImageViews      []*ImageRequest `json:"imageViews"`
	VideoViews      *VideoRequest   `json:"videoViews"`
	Comment         string          `json:"comment"`
	RelatedTeachers []string        `json:"relatedTeachers,optional,omitempty"`
}

type ImageRequest {
	Original  string `json:"original,omitempty,optional"`
	Preview   string `json:"preview,omitempty,optional"`
	Thumbnail string `json:"thumbnail,omitempty,optional"`
	Size      int    `json:"size,omitempty,optional"`
	Height    int    `json:"height,omitempty,optional"`
	Width     int    `json:"width,omitempty,optional"`
}

type VideoRequest {
	Title        string `json:"title,omitempty,optional"`
	Desc         string `json:"desc,omitempty,optional"`
	CoverUrl     string `json:"coverUrl,omitempty,optional"`
	PlayUrl      string `json:"playUrl,omitempty,optional"`
	PlayTime     int    `json:"playTime,omitempty,optional"`
	MagicContext string `json:"magicContext,omitempty,optional"`
	Type         string `json:"type,omitempty,optional"`
	Height       int    `json:"height,omitempty,optional"`
	Width        int    `json:"width,omitempty,optional"`
}

type TaskListRequest {
	TaskName         string `json:"taskName,optional"`            // 搜索关键字
	NowPage          int64  `json:"nowPage,optional,default=1"`   // 当前页码
	PageSize         int64  `json:"pageSize,optional,default=12"` // 每页条数
	IsEdit           bool   `json:"isEdit,optional"`
