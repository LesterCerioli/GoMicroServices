@server(
	jwt: Auth
	group: evaluate
)
service classevaluation-api {
	@doc(
		summary: "web端获取可评价任务列表"
	)
	@server(
		handler: TaskEvaluateListHandler
	)
	get /classevaluation/do-task/list () returns (EvaluationTaskListResponse)
	
	@doc(
		summary: "获取需要评价的表格"
	)
	@server(
		handler: TaskEvaluateGridHandler
	)
	post /classevaluation/do-evaluate/grid (EvaluateGridRequest) returns (EvaluateGridResponse)
	
	@doc(
		summary: "当前内容下所有评价项"
	)
	@server(
		handler: TaskEvaluateItemListHandler
	)
	get /classevaluation/do-item/list/:contentId (EvaluateIndicatorContentItemRequest) returns (EvaluateIndicatorContentItemResponse)
	
	@doc(
		summary: "删除指标任务清单"
	)
	@server(
		handler: IndicatorOptionDelHandler
	)
	get /classevaluation/indicatorOption/del (IndicatorOptionRequest) returns (BoolResponse)
	
	@doc(
		summary: "当前评价任务配置项展示"
	)
	@server(
		handler: TaskEvaluateConfigListHandler
	)
	post /classevaluation/do-config/info (EvaluateDetailRequest) returns (EvaluateDetailResponse)
	
	@doc(
		summary: "获取当前评价对象的关联关系"
	)
	@server(
		handler: TaskEvaluateObjectRelationHandler
	)
	post /classevaluation/object-relation (ObjectRelationRequest) returns (ObjectRelationResponse)
	
	@doc(
		summary: "新增评价"
	)
	@server(
		handler: TaskEvaluateLogV3Handler
	)
	post /classevaluation/do-evaluate/upset (EvaluateAddLogRequestV3) returns (EvaluateAddLogResponseV3)
	
	@doc(
		summary: "地点学生关联列表"
	)
	@server(
		handler: PlaceStudentRelationHandler
	)
	get /classevaluation/place-relation/:placeId(PlaceStudentRelationRequest) returns(PlaceStudentRelationResponse)
	
	@doc(
		summary: "评价内容记录"
	)
	@server(
		handler: EvaluatedContentLogHandler
	)
	post /classevaluation/data/evaluated-content-log(EvaluatedContentLogRequest) returns(EvaluatedLogResponse)
	
	@doc(
		summary: "删除评价"
	)
	@server(
		handler: WebDeleteEvaluateLogHandler
	)
	delete /classevaluation/do-evaluate/delete/:detailId(EvaluateDetailActRequest) returns(BoolResponse)
	
	@doc(
		summary: "判断老师班级考核的权限"
	)
	@server(
		handler: WebJudgeClassEvalAuthorityHandler
	)
	get /classevaluation/auth() returns(TeacherAuthResponse)
	
	@doc(
		summary: "所有评价对象评价汇总表"
	)
	@server(
		handler: EvaluateRecordGridHandler
	)
	post /classevaluation/evaluate-record/grid(EvaluateRecordGridRequest) returns(EvaluateRecordGridResponse)
	
	@doc(
		summary: "单个评价对象评价内容汇总表"
	)
	@server(
		handler: EvaluatedObjectSummaryHandler
	)
	post /classevaluation/evaluated-object/summary(EvaluatedObjectSummaryRequest) returns(EvaluatedObjectSummaryResponse)
	
	@doc(
		summary: "单个评价对象评价记录"
	)
	@server(
		handler: EvaluatedObjectLogHandler
	)
	post /classevaluation/evaluated-object/log(EvaluatedObjectLogRequest) returns(EvaluatedObjectLogResponse)
}

