type EvaluateTaskIndicator {
	IndicatorId    int64                  `json:"indicatorId,optional"` // 新建则不传
	IndicatorName  string                 `json:"indicatorName,optional"`
	BaseScore      int64                  `json:"baseScore,optional"`
	Status         int                    `json:"status,omitempty,optional"` // 不处理:0 insert:1 update:2 delete:3
	EvaluateType   int                    `json:"evaluateType,optional"`     // 评价类型 1班级 2教师 3地点
	EvaluateObject []*EvaluateObjectInfo  `json:"evaluateObject,optional"`   // 评价对象信息
	Contents       []*EvaluateTaskContent `json:"contents,optional"`
}

type EvaluateObjectInfo {
	OptionId        int64             `json:"optionId"`
