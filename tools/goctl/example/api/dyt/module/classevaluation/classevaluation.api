}

type TaskViewResponse {
	TaskId              int64                `json:"taskId"`        // 任务ID
	TaskName            string               `json:"taskName"`      // 任务名称
	CalCycle            int                  `json:"calCycle"`      // 计算周期 1.每月 2.每周 3.每天
