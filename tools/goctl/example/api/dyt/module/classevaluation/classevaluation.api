	Evaluator       []int64           `json:"evaluator"`              // 评价人
	EvaluatedIds    []int64           `json:"evaluatedIds"`           // 被评价对象的id
	EvaluateWeekDay []int64           `json:"evaluateWeekDay"`        // 被评价周日期  0-6
	EvaluateTime    []*EvaluateTime   `json:"evaluateTime,optional"`
