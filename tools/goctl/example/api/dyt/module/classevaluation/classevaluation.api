	Sort            int64                      `json:"sort"`             // 排序
}

type EvaluateTaskContentItem {
	ItemId        int64  `json:"itemId,optional"`       // 评价项ID 新增时可为空 编辑时必填
	ItemName      string `json:"itemName"`              // 评价项名称
	ItemOrder     int64  `json:"itemOrder"`             // 排序 1，2，3...
	ItemScoreType int64  `json:"itemScoreType"`         // 是否为固定分数 0 固定 1浮动
	ItemScore     int64  `json:"itemScore,optional"`    // 评价项分值
	ItemScoreMin  int64  `json:"itemScoreMin,optional"` // 评价项分值最小值
	ItemScoreMax  int64  `json:"itemScoreMax,optional"` // 评价项分值最大值
	IsDefault     int64  `json:"isDefault"`             // 是否默认 1 是 0 否
	Status        int    `json:"status,omitempty"`      // 不处理:0 insert:1 update:2 delete:3
}

