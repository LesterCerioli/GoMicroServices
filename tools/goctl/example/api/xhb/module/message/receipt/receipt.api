type ReceiptMessageRequest {
	Token     string `path:"token"`
	MessageId string `json:"messageId"`
	Attitude  string `json:"attitude"`
}

type ReceiptMessageResponse {
	Successful bool `json:"successful"`
}

service receipt-api {
	@doc(
		summary: 发送消息回执(赞,打勾,疑问,收到)
	)
	@server(
		handler: receiptMessageHandler
	)
	post /message/receipt/:token(ReceiptMessageRequest) returns(ReceiptMessageResponse)
}