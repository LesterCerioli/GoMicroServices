type UserSurveyInfoRequest {
	Token    string `path:"token"`
	SurveyId string `path:"surveyId"`
}

type UserSurveyInfoResponse {
	UserId     string   `json:"userId,omitempty"`
	OptionKeys []string `json:"optionKeys,omitempty"`
}

service survey-api {
	@doc(
		summary: 获取某个用户的投票情况
	)
	@server(
		handler: UserSurveyInfoHandler
	)
	post /survey/view/:token/:surveyId(UserSurveyInfoRequest) returns(UserSurveyInfoResponse)
}