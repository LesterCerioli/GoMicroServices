type ClassTargetDataListItemV2 {
	TargetId            int64                    `json:"targetId"`
	TargetName          string                   `json:"targetName"`
	TargetItemNames     []string                 `json:"targetItemNames"`        // 评价指标命数组
	ClassTargetDataList []*ClassTypeDataListItem `json:"scopeClassTypeDataList"` // 每个班级在 这个指标下面的情况
}

type AssessTargetUpdateRequest {
	TargetId   int64                   `json:"targetId"`
	CampusId   int64                   `json:"-,optional"`
	TargetName string                  `json:"targetName"` // 新的指标名
	StuType    int64                   `json:"stuType"`    // 是否可选学生是否可选学生
	ItemList   []*TargetItem           `json:"itemList"`   // 这个指标下所有的item
	ChangeList []*TargetItemNameChange `json:"changeList"` // 如果这个指标是被使用的,就必须传传这个数据修改名字的list
}

type TargetItem {
	TargetItemName  string `json:"targetItemName"`
	TargetItemScore int64  `json:"targetItemScore,optional"`
}

type TargetItemNameChange {
	OldItemName string `json:"oldItemName"`
	NewItemName string `json:"newItemName"`
}

type UpdateUseTypeRequest {
	CampusId      int64          `json:"-,optional"` // 学校id
	TypeName      string         `json:"typeName"`   // 当前的typename
	TypeId        int64          `json:"typeId"`
	AssessClasses []*AssessClass `json:"assessClasses"` // 新增的或者有修改的班级范围
	AssessSites   []*AssessSite  `json:"assessSites"`   // 新增的或者有修改的自定义范围
	Targets       []*OptTarget   `json:"targets"`       // 新增的targetId
}

type AssessClass {
	ClassId      int64            `json:"classId"`  // 班级id
	Operator     []int64          `json:"operator"` // 老师id
	AssessObject *AddAssessObject `json:"assessObject,optional"`
}

type AssessObject {
	AssessObjectId   int64  `json:"assessObjectId"` // 评价对象id
	AssessObjectName string `json:"assessObjectName"`
}

type AssessSite {
	SiteName     string           `json:"siteName"` // 自定义评价范围名
	AssessObject *AddAssessObject `json:"assessObject"`
	Operator     []int64          `json:"operator"`
}

type AddAssessObject {
	AssessObjectId   int64  `json:"objectId"`
	AssessObjectName string `json:"objectName"`
}

type OptTarget {
	TargetId   int64  `json:"targetId"`
	TargetName string `json:"targetName"`
}

type AssessTargetItem {
	AssessTargetItemId    int64  `json:"assessTargetItemId"`
	AssessTargetItemName  string `json:"assessTargetItemName"`
	AssessTargetItemScore int64  `json:"assessTargetItemScore"`
}

type StudentOtherDetail {
	StudentId   int64  `json:"studentId"`
	StudentName string `json:"studentName"`
}

type MyAssessDetailRequest {
}

type AssessTaskTypeScanSaveRequest {
}

type BoolResponse {
}

@server(
	host:0.0.0
	port: 8080
)
service teacherassess-api {
	@doc(
		summary: 新增评价组
	)
	@server(
		handler: AssessObjectAddHandler
	)
	post /teacherassess/object/add(AddObjectRequest)
	
	@doc(
		summary: 编辑评价组
	)
	@server(
		handler: AssessObjectUpdateHandler
	)
	post /teacherassess/object/update(UpdateObjectRequest)
	
	@doc(
		summary: 评价任务信息
	)
	@server(
		handler: AssessTaskInfoHandler
	)
	post /teacherassess/task/info(AssessTaskInfoRequest) returns(TaskInfoResponse)
	
	@doc(
		summary: 新增评价任务
	)
	@server(
		handler: AssessTaskAddHandler
	)
	post /teacherassess/task/add(AddTaskRequest)
	
	@doc(
		summary: 编辑评价任务
	)
	@server(
		handler: AssessTaskUpdateHandler
	)
	post /teacherassess/task/update(UpdateTaskRequest)
	
	@doc(
		summary: 保存评价
	)
	@server(
		handler: AssessTaskTypeSaveHandler
	)
	post /teacherassess/h5/task-type/save(AssessTaskTypeSaveRequest)
	
	@doc(
		summary: 我的评价记录
	)
	@server(
		handler: MyAssessDetailHandler
	)
	post /teacherassess/h5/task-type/self(MyAssessDetailRequest)
	
	@doc(
		summary: 教师可评价列表
	)
	@server(
		handler: TaskTypeList
	)
	get /teacherassess/h5/task-type/list() returns(TaskTypeList)
	
	@doc(
		summary: pc端老师评价数据汇总
	)
	@server(
		handler: AssessTeacherDataHandler
	)
	post /teacherassess/teacherAnalysis/assessTeacherData(AssessTeacherDataRequest) returns(AssessTeacherDataResponse)
	
	@doc(
		summary: pc端老师评价assessType分析
	)
	@server(
		handler: TeacherDataTypeHandler
	)
	post /teacherassess/teacherAnalysis/teacherDataType(TeacherDataTypeRequest) returns(TeacherDataTypeResponse)
	
	@doc(
		summary: pc端班级评价数据汇总
	)
	@server(
		handler: AssessClassDataHandler
	)
	post /teacherassess/classAnalysis/assessClassData(AssessClassDataRequest) returns(AssessClassDataResponse)
	
	@doc(
		summary: pc端班级评价assessType分析
	)
	@server(
		handler: ClassDataTypeHandler
	)
	post /teacherassess/classAnalysis/classDataType(ClassDataTypeRequest) returns(ClassDataTypeV2Response)
	
	@doc(
		summary: h5端老师排行榜数据
	)
	@server(
		handler: TeacherRankDataHandler
	)
	post /teacherassess/h5/teacherAnalysis/teacherRankData(TeacherRankDataRequest) returns(TeacherRanKDataResponse)
	
	@doc(
		summary: h5端老师评价分类数据
	)
	@server(
		handler: TeacherTaskTypeDetailHandler
	)
	post /teacherassess/h5/teacherAnalysis/teacherTaskTypeDetail(TeacherTaskDetailDataRequest) returns(TeacherTaskDetailDataResponse)
	
	@doc(
		summary: h5端老师评价指标数据
	)
	@server(
		handler: TeacherTaskTypeTargetDetailHandler
	)
	post /teacherassess/h5/teacherAnalysis/teacherTaskTypeTargetDetail(TeacherTaskTypeTargetDetailRequest) returns(TeacherTaskTypeTargetDetailResponse)
	
	@doc(
		summary: h5端班级排行榜数据
	)
	@server(
		handler: ClassRankDataHandler
	)
	post /teacherassess/h5/classAnalysis/classRankData(ClassAssessRankDataRequest) returns(ClassAssessRanKDataResponse)
	
	@doc(
		summary: h5端班级评价分类数据
	)
	@server(
		handler: ClassTaskTypeDetailHandler
	)
	post /teacherassess/h5/classAnalysis/classTaskTypeDetail(ClassTaskDetailDataRequest) returns(ClassTaskDetailDataResponse)
	
	@doc(
		summary: h5端班级评价指标数据
	)
	@server(
		handler: ClassTaskTypeTargetDetailHandler
	)
	post /teacherassess/h5/classAnalysis/classTaskTypeTargetDetail(ClassTaskTypeTargetDetailRequest) returns(ClassTaskTypeTargetDetailResponse)
	
	@doc(
		summary: 学生列表
	)
	@server(
		handler: AssessStudentListHandler
	)
	post /student/h5/list(AssessStudentListRequest) returns(AssessStudentListResponse)
	
	@doc(
		summary: 扫描评价（通过二维码与 nfc 入口）
	)
	@server(
		handler: AssessTaskTypeScanSaveHandler
	)
	post /teacherassess/h5/task-type/scan-save(AssessTaskTypeScanSaveRequest)
	
	@doc(
		summary: 教师-学生可评价列表
	)
	@server(
		handler: OtherAssessList
	)
	post /teacherassess/h5/otherassess/list(OtherAssessRequest) returns(OtherAssessResponse)
	
	@doc(
		summary: 获取评价组信息
	)
	@server(
		handler: ObjectInfoHandler
	)
	post /teacherassess/object/info(GetObjectRequest) returns(GetObjectResponse)
	
	@doc(
		summary:老师某任务下面的评价详情
	)
	@server(
		handler: TeacherTaskDetail
	)
	post /teacherassess/h5/teacherAnalysis/teachertaskdetail(TeacherTaskDetailRequest) returns(TeacherTaskDetailResponse)
	
	@doc(
		summary:通过taskid获得typelist
	)
	@server(
		handler: TaskTypeH5
	)
	post /teacherassess/h5/task/typelist(TaskTypeRequest) returns(TaskTypeResponse)
	
	@doc(
		summary:通过taskid获得typelist
	)
	@server(
		handler: TaskType
	)
	post /teacherassess/task/typelist(TaskTypeRequest) returns(TaskTypeResponse)
	
	@doc(
		summary: 教师-学生评价页详情
	)
	@server(
		handler: OtherAssessDetailHandler
	)
	post /teacherassess/h5/otherassess/detail(OtherAssessDetailRequest) returns(OtherAssessDetailResponse)
	
	@doc(
		summary: 教师-学生评价保存
	)
	@server(
		handler: OtherAssessSaveHandler
	)
	post /teacherassess/h5/otherassess/save(OtherAssessSaveRequest) returns(BoolResponse)
	
	@doc(
		summary: 修改指标
	)
	@server(
		handler: AssessTargetUpdate
	)
	post /teacherassess/target/update(AssessTargetUpdateRequest)
	
	@doc(
		summary: 修改正在使用的type
	)
	@server(
		handler: AssessTypeUseUpdate
	)
	post /teacherassess/type/update-use(UpdateUseTypeRequest)
}