	SundayTeachers    []int64 `json:"sundayTeachers,optional"`
	WeekTeachers      []int64 `json:"weekTeachers,optional"` // 固定评价组被评价教师id
	Type              int64   `json:"type"`                  // 评价组类型。0-自定义评价组，1-固定评价组
}

