type VoiceEvaluateRequest {
	Text string `json:"text"`
	Type int    `json:"type,optional"`
}

type EvaluateOption {
	Id        int64  `json:"id,omitempty"`
	Name      string `json:"name,omitempty"`
	ClassName string `json:"className,omitempty"`
	Type      int    `json:"type,omitempty"`
}

type VoiceEvaluateResponse {
	TaskId                int64                           `json:"taskId"`                  // 任务 id
	TaskName              string                          `json:"taskName,omitempty"`      // 任务名称
	IndicatorId           int64                           `json:"indicatorId"`             // 指标 id
	IndicatorName         string                          `json:"indicatorName,omitempty"` // 指标名称
	ContentId             int64                           `json:"contentId"`               // 内容 id
	ContentName           string                          `json:"contentName,omitempty"`   // 内容名称
	EvaluateType          int                             `json:"evaluateType"`            // 评价类型
	EvaluateId            int64                           `json:"evaluateId,omitempty"`    // 评价人 id
	EvaluateName          string                          `json:"evaluateName,omitempty"`  // 评价人显示名称
	EvaluateOptions       []*EvaluateOption               `json:"evaluateOptions,omitempty"`
	ContentItem           []*EvaluateIndicatorContentItem `json:"contentItem,omitempty"`           // 评价项
	SelectedContentItemId int                             `json:"selectedContentItemId,omitempty"` // 匹配到评价项的选项 -1：未匹配到
	TeacherEvaluateCan    int                             `json:"teacherEvaluateCan"`              // 如果是老师类型 0 就代表今天不能评价 1代表可以
}

type AllEvaluateLogsRequest {
	TaskId      int64 `json:"taskId"`               // 任务id
	EvaluatorId int64 `json:"evaluatorId,optional"` // 评价人id
	StartTime   int64 `json:"startTime"`            // 开始时间
	EndTime     int64 `json:"endTime"`              // 结束时间
	NowPage     int   `json:"nowPage"`
	PageSize    int   `json:"pageSize"`
}

type AllEvaluateLogsResponse {
	Total   int64      `json:"total"`   // 总计
	NowPage int64      `json:"nowPage"` // 当前页
	List    []*LogItem `json:"list"`
}

type LogItem {
	Id              int64              `json:"id"`
	TaskId          int64              `json:"taskId"`                  // 任务id
	TaskName        string             `json:"taskName"`                // 任务名
	Avatar          string             `json:"avatar"`                  // 图标
	EvaluatedTime   int64              `json:"evaluatedTime"`           // 评价时间
	EvaluatorName   string             `json:"evaluatorName"`           // 评价人名
	EvaluateType    int                `json:"evaluateType"`            // 评价类型:1-班级;2-教师;3-地点
	EvaluatedId     int64              `json:"evaluatedId"`             // 被评价人id
	EvaluatedName   string             `json:"evaluatedName"`           // 被评价人名
	IndicatorId     int64              `json:"indicatorId"`             // 指标id
	IndicatorName   string             `json:"indicatorName"`           // 指标名
	ContentId       int64              `json:"contentId"`               // 内容Id
	ContentName     string             `json:"contentName"`             // 内容名
	SelectType      int64              `json:"selectType"`              // 1单选 2多选
	ContentItemId   int64              `json:"contentItemId"`           // 评价项id
	ContentItemName string             `json:"contentItemName"`         // 评价项
	Score           int64              `json:"score"`                   // 分数
	RelTeachers     string             `json:"relTeachers,omitempty"`   // 相关教师姓名
	RelTeacherIds   []int64            `json:"relTeacherIds,omitempty"` // 相关教师id
	RelStudents     string             `json:"relStudents,omitempty"`   // 相关学生姓名
	RelStudentIds   []int64            `json:"relStudentIds,omitempty"` // 相关学生id
	RelClasses      []*ObjectClassInfo `json:"relClasses,omitempty"`    // 相关班级信息
	Remark          *CommentDetail     `json:"remark,omitempty"`        // 评价内容
}

type UpdateIndicatorRequest {
	TaskId        int64  `json:"taskId"`        // 任务id
	IndicatorId   int64  `json:"indicatorId"`   // 指标id'
	IndicatorName string `json:"indicatorName"` // 指标名
	EvaluateType  int    `json:"evaluateType"`  // 评价类型 1班级 2教师 3地点
	BaseScore     int64  `json:"baseScore"`     // 分数
}

type ActContentRequest {
	IndicatorId int64                `json:"indicatorId"` // 指标id
	Content     *EvaluateTaskContent `json:"content"`     // 内容
}

type ActObjectRequest {
	IndicatorId int64         `json:"indicatorId"` // 指标id
	Object      *ObjectConfig `json:"object"`      // 评价关系
}

type ObjectConfig {
	ObjectId        int64           `json:"objectId"`
	EvaluateType    int             `json:"evaluateType,optional"` // 评价类型 1班级 2教师 3地点
	Evaluator       []int64         `json:"evaluator"`             // 评价人
	EvaluatedIds    []int64         `json:"evaluatedIds"`          // 被评价对象的id
	EvaluateWeekDay []int64         `json:"evaluateWeekDay"`       // 被评价周日期  0-6
	EvaluateTime    []*EvaluateTime `json:"evaluateTime"`          // 评价时间
	Status          int             `json:"status,omitempty"`      // 不处理:0 insert:1 update:2 delete:3
}

type IndicatorOptionStopRequest {
	IndicatorOptionId int64 `json:"indicatorOptionId"`
}

