type TaskListRequest {
	TaskName         string `json:"taskName,optional"`            // 搜索关键字
	NowPage          int64  `json:"nowPage,optional,default=1"`   // 当前页码
	PageSize         int64  `json:"pageSize,optional,default=12"` // 每页条数
	IsEdit           bool   `json:"isEdit,optional"`
	NoEmptyIndicator bool   `json:"noEmptyIndicator,optional"` //true 不包含指标为空的任务， false 包含相关任务
}

type TaskListResponse {
	Total    int64       `json:"total"`    // 总数量
	TaskList []*TaskInfo `json:"taskList"` // 任务列表
}

type TaskInfo {
	Id              int64  `json:"id"`              // 任务ID
	TaskName        string `json:"taskName"`        // 任务名称
	Indicators      string `json:"indicators"`      // 当前任务下评价指标
	EvaluationTypes []int  `json:"evaluationTypes"` // 当前任务下包含的指标类型
	SyncAppraisal   int64  `json:"syncAppraisal"`   // 是否同步学生评价 1同步 0不同步
}

type TaskViewRequest {
	TaskId int64 `path:"taskId"` // 任务ID
}

type TaskViewResponse {
	TaskId              int64                `json:"taskId"`        // 任务ID
	TaskName            string               `json:"taskName"`      // 任务名称
	CalCycle            int                  `json:"calCycle"`      // 计算周期 1.每月 2.每周 3.每天
	IndicatorList       []*TaskIndicatorInfo `json:"indicatorList"` // 指标列表
	ConfigIdList        []int64              `json:"configIdList"`  // 配置列表
	Managers            []*TeacherInfo       `json:"managers"`      // 管理者列表
	ClassIndicatorIds   []int64              `json:"classIndicatorIds"`
	TeacherIndicatorIds []int64              `json:"teacherIndicatorIds"`
	ScopeIndicatorIds   []int64              `json:"scopeIndicatorIds"`
	Avatars             []string             `json:"avatars"`
	CheckedAvatar       int64                `json:"checkedAvatar"`
	Frequency           int64                `json:"frequency"` // 任务频次  0:日 1:周 2:月
}

type TaskIndicatorInfo {
	IndicatorId   int64  `json:"indicatorId"`   // 指标ID
	IndicatorName string `json:"indicatorName"` // 指标名称
	BaseScore     int64  `json:"baseScore"`     // 基础分数
	Sort          int64  `json:"sort,optional"`
}

type TaskAddRequest {
	TaskName      string               `json:"taskName"`           // 任务名称
	CalCycle      int                  `json:"calCycle"`           // 计算周期 1.每月 2.每周 3.每天
	IndicatorList []*TaskIndicatorInfo `json:"indicatorList"`      // 指标列表
	ConfigIdList  []int64              `json:"configIdList"`       // 配置ID列表
	Managers      []*TeacherInfo       `json:"managers,optional"`  // 管理者列表
	Avatar        string               `json:"avatar,optional"`    // 任务图标
	Frequency     int64                `json:"frequency,optional"` // 任务频次  0:日 1:周 2:月
}

type TaskEditRequest {
	TaskId        int64                `json:"taskId"`                 // 任务ID
	TaskName      string               `json:"taskName"`               // 任务名称
	CalCycle      int                  `json:"calCycle"`               // 计算周期 1.每月 2.每周 3.每天
	IndicatorList []*TaskIndicatorInfo `json:"indicatorList,optional"` // 指标列表
	ConfigIdList  []int64              `json:"configIdList"`           // 配置ID列表
	Managers      []*TeacherInfo       `json:"managers,optional"`      // 管理者列表
	Avatar        string               `json:"avatar,optional"`        // 任务图标
	Frequency     int64                `json:"frequency,optional"`     // 任务频次  0:日 1:周 2:月
