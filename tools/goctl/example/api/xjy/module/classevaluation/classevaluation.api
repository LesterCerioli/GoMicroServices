type IndicatorOptions {
	OptionsId              int64                    `json:"optionsId,optional"`                    // 评价关系ID
	Evaluators             []*TeacherInfo           `json:"evaluators"`                            // 评价人列表
	EvaluatedTeachers      []*EvaluatedTeachersInfo `json:"evaluatedTeachers,omitempty,optional"`  // 评价对象 被评价老师列表
	EvaluatedTeacherIsSame int64                    `json:"evaluatedTeacherIsSame,optional"`       // 评价对象 被评价老师是否每天一样 1一样 0不一样
	EvaluatedClassList     []*EvaluatedClassInfo    `json:"evaluatedClassList,omitempty,optional"` // 评价对象 被评价班级列表
	EvaluatedScopes        []int64                  `json:"evaluatedScopes,omitempty,optional"`    // 评价对象 被评价场地列表
	EvaluatedScopeNames    []string                 `json:"evaluatedScopeNames,omitempty,optional"`
}

type EvaluatedClassInfo {
	ClassId   int64  `json:"classId"`   // 被评价班级ID
	ClassName string `json:"className"` // 被评价班级名称
}

type EvaluatedTeachersInfo {
	EvaluateWeekDay      int            `json:"evaluateWeekDay"`      // 被评价周日期
